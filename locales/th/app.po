msgstr "ล้างแคช"

msgctxt "label_menu_history"
msgid "History"
msgstr "ประวัติ"

msgctxt "label_menu_share"
msgid "Share"
msgstr "แบ่งปัน"

msgctxt "label_menu_my_shots"
msgid "My shots"
msgstr "ภาพหน้าจอของฉัน"

